<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to root_path, class: "navbar-brand" do %>
      <h4><i class="fas fa-motorcycle"></i> Air Beers & Bikes</h4>
  <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
        <% if user_signed_in? %>
        <li>
          <p class="nav-link"><strong><%= current_user.email.split('@').first %></strong></p>
        </li class="nav-item">
        <li class="nav-item dropdown">
          <%= image_tag "data:image/jpeg;base64,", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
            <%= link_to "My Bookings", bookings_path, class: "dropdown-item" %>
            <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
          </div>
        </li>
      <% else %>
        <li class="nav-item">
          <%= link_to "Login", new_user_session_path, class: "btn btn-primary m-1" %>
        </li>
        <li class="nav-item">
          <%= link_to "Sign In", new_user_registration_path, class: "btn btn-primary m-1" %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
