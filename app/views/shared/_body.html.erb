<div class="bikes-container m-5">

  <h4>Locations <i class="fas fa-map-marker-alt"></i> </h4>
  <div class="row cards">
    <div class="col-4">
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://images.unsplash.com/photo-1546375982-c22276aa12f0?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60)">
        <%= link_to "Lisboa", bikes_path, class: "nav-link" %>
      </div>
    </div>

    <div class="col-4">
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://images.unsplash.com/photo-1513735492246-483525079686?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60)">
        <%= link_to "Porto", bikes_path, class: "nav-link" %>
      </div>
    </div>

    <div class="col-4">
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(data:image/jpeg;base64,)">
        <%= link_to "Luanda", bikes_path, class: "nav-link" %>
      </div>
    </div>
  </div>


  <h4>Bikes <i class="fas fa-motorcycle"></i></h4>
  <div class="row">
    <div class="cards-list">
      <% @bikes.each do |bike| %>
        <%= link_to bike_path(bike) do %>
          <div class="card" style="width: 18rem;">
            <%= cl_image_tag bike.photo.key, class: "card-img-top"  %>
            <div class="card-body">
              <h6><%= "#{bike.brand} #{bike.model}" %></h6>
            </div>
          </div>
        <% end %>
      <% end %>
    </div>
  </div>


</div>
